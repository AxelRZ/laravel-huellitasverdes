        "sass-loader": "^8.0.2",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-react-app": "^3.1.2",
        "owl.carousel": "^2.3.4",
        "tailwindcss": "^1.8.10"
    }
